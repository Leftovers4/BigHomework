<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root prefHeight="690.0" prefWidth="850.0" styleClass="panel" stylesheets="@../../css/hotelworker/hotelworkerstyle.css" type="Pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="presentation.hotelworkerui.hotelworkercontroller.RegisterRoomPaneController">
    <children>
        <HBox id="topBar" prefHeight="50.0" prefWidth="850.0" stylesheets="@../../css/user/userstyle.css">
            <children>
                <Button mnemonicParsing="false" stylesheets="@../../css/user/userstyle.css">
                    <graphic>
                        <ImageView fitHeight="40.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/back.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Button>
                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../img/hotelworker/initialPhoto.png" />
                    </image>
                    <HBox.margin>
                        <Insets left="480.0" top="10.0" />
                    </HBox.margin>
                </ImageView>
                <SplitMenuButton maxWidth="120.0" mnemonicParsing="false" text="name">
                    <items>
                        <MenuItem mnemonicParsing="false" text="退出登录" />
                    </items>
                    <HBox.margin>
                        <Insets left="10.0" top="8.0" />
                    </HBox.margin>
                </SplitMenuButton>
                <Button mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/minimize.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets left="60.0" top="5.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/exit.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets top="5.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <TableView id="listTable" fx:id="listTable" layoutX="125.0" layoutY="203.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="200.0" prefWidth="600.0">
            <columns>
                <TableColumn id="idCol" fx:id="idCol" editable="false" prefWidth="200.0" text="类型" />
                <TableColumn id="userCol" fx:id="userCol" editable="false" prefWidth="200.0" text="数量" />
                <TableColumn id="amountCol" fx:id="amountCol" editable="false" prefWidth="200.0" text="原价（元/天）" />
            </columns>
        </TableView>
        <HBox id="listTopBar" fx:id="listTopBar" alignment="CENTER_LEFT" layoutX="125.0" layoutY="151.0" prefHeight="52.0" prefWidth="600.0">
         <children>
            <Button fx:id="addBtn" mnemonicParsing="false" text="增加">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="deleteBtn" layoutX="10.0" layoutY="18.0" mnemonicParsing="false" text="删除">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="editBtn" layoutX="162.0" layoutY="18.0" mnemonicParsing="false" text="修改">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
        </HBox>
      <Button id="saveRegisterBtn" fx:id="saveRegisterBtn" layoutX="571.0" layoutY="570.0" mnemonicParsing="false" text="保存" />
      <Button id="cancelRegisterBtn" fx:id="cancelRegisterBtn" layoutX="665.0" layoutY="570.0" mnemonicParsing="false" text="取消" />
    </children>
</fx:root>
