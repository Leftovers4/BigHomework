<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root prefHeight="690.0" prefWidth="850.0" styleClass="panel" stylesheets="@../../css/hotelworker/hotelworkerstyle.css" type="Pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox id="topBar" prefHeight="50.0" prefWidth="850.0" stylesheets="@../../css/user/userstyle.css">
            <children>
                <Button mnemonicParsing="false" stylesheets="@../../css/user/userstyle.css">
                    <graphic>
                        <ImageView fitHeight="40.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/back.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Button>
                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../img/hotelworker/initialPhoto.png" />
                    </image>
                    <HBox.margin>
                        <Insets left="480.0" top="10.0" />
                    </HBox.margin>
                </ImageView>
                <SplitMenuButton maxWidth="120.0" mnemonicParsing="false" text="name">
                    <items>
                        <MenuItem mnemonicParsing="false" text="退出登录" />
                    </items>
                    <HBox.margin>
                        <Insets left="10.0" top="8.0" />
                    </HBox.margin>
                </SplitMenuButton>
                <Button mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/minimize.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets left="60.0" top="5.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/exit.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets top="5.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
      <TabPane fx:id="tabPane" layoutX="64.0" layoutY="142.0" prefHeight="289.0" prefWidth="711.0" tabClosingPolicy="UNAVAILABLE" tabMinWidth="150.0">
        <tabs>
          <Tab fx:id="allOrderTab" text="所有订单">
               <content>
                  <TableView fx:id="allOrderTable" prefHeight="244.0" prefWidth="645.0">
                    <columns>
                      <TableColumn prefWidth="150.0" text="订单号" />
                      <TableColumn prefWidth="130.0" text="客户" />
                        <TableColumn prefWidth="150.0" text="生成时间" />
                        <TableColumn prefWidth="150.0" text="订单状态" />
                        <TableColumn prefWidth="130.0" />
                    </columns>
                  </TableView>
               </content>
          </Tab>
          <Tab fx:id="unExeOrderTab" text="未执行订单">
               <content>
                  <TableView prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn prefWidth="150.0" text="订单号" />
                      <TableColumn prefWidth="130.0" text="客户" />
                        <TableColumn prefWidth="150.0" text="预计入住时间" />
                        <TableColumn prefWidth="150.0" text="最晚执行时间" />
                        <TableColumn prefWidth="130.0" />
                    </columns>
                  </TableView>
               </content>
          </Tab>
            <Tab fx:id="abnormalOrderTab" text="异常订单">
               <content>
                  <TableView prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn prefWidth="200.0" text="订单号" />
                      <TableColumn prefWidth="130.0" text="客户" />
                        <TableColumn prefWidth="200.0" text="最晚执行时间" />
                        <TableColumn prefWidth="180.0" />
                    </columns>
                  </TableView>
               </content>
            </Tab>
            <Tab fx:id="exeOrderTab" text="已执行订单">
               <content>
                  <TableView prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn prefWidth="150.0" text="订单号" />
                      <TableColumn prefWidth="130.0" text="客户" />
                        <TableColumn prefWidth="150.0" text="入住时间" />
                        <TableColumn prefWidth="150.0" text="评分" />
                        <TableColumn prefWidth="130.0" />
                    </columns>
                  </TableView>
               </content>
            </Tab>
        </tabs>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TabPane>
    </children>
</fx:root>
