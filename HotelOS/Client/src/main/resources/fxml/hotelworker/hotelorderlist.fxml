            <TextField id="searchField" fx:id="searchField" />
            <ComboBox promptText="所有订单">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </HBox>
