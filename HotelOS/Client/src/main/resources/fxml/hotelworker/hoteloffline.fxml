<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root prefHeight="690.0" prefWidth="850.0" styleClass="panel" stylesheets="@../../css/hotelworker/hotelworkerstyle.css" type="Pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.hotelworkerui.hotelworkercontroller.UpdateCheckInPaneController">
    <children>
        <HBox id="topBar" prefHeight="50.0" prefWidth="850.0" stylesheets="@../../css/user/userstyle.css">
            <children>
                <Button fx:id="backBtn" mnemonicParsing="false" onAction="#back" stylesheets="@../../css/user/userstyle.css">
                    <graphic>
                        <ImageView fitHeight="40.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/back.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Button>
                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../img/hotelworker/initialPhoto.png" />
                    </image>
                    <HBox.margin>
                        <Insets left="480.0" top="10.0" />
                    </HBox.margin>
                </ImageView>
                <SplitMenuButton maxWidth="120.0" mnemonicParsing="false" text="name">
                    <items>
                        <MenuItem mnemonicParsing="false" text="退出登录" />
                    </items>
                    <HBox.margin>
                        <Insets left="10.0" top="8.0" />
                    </HBox.margin>
                </SplitMenuButton>
                <Button fx:id="minWindowBtn" mnemonicParsing="false" onAction="#minWindow">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/minimize.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets left="60.0" top="5.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="closeWindowBtn" mnemonicParsing="false" onAction="#closeWindow">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../img/common/exit.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets top="5.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
      <VBox layoutX="108.0" layoutY="172.0" prefHeight="200.0" prefWidth="634.0">
         <children>
            <TableView prefHeight="200.0" prefWidth="285.0">
              <columns>
                <TableColumn prefWidth="141.0" text="客房类型" />
                <TableColumn prefWidth="110.0" text="数量" />
                  <TableColumn prefWidth="382.0" text="房间号" />
              </columns>
            </TableView>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <HBox alignment="CENTER_RIGHT">
                     <children>
                        <ComboBox prefHeight="36.0" prefWidth="140.0" promptText="客房类型" />
                        <ComboBox prefHeight="36.0" prefWidth="100.0" promptText="数量">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </ComboBox>
                        <TextField prefHeight="36.0" prefWidth="212.0" promptText="房间号">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <Pane>
                     <children>
                        <Button layoutX="95.0" layoutY="7.0" mnemonicParsing="false" text="删除" />
                        <Button layoutX="25.0" layoutY="7.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="60.0" text="增加" />
                        <Button layoutX="25.0" layoutY="7.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="60.0" text="确认" visible="false" />
                        <Button layoutX="95.0" layoutY="7.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="60.0" text="取消" visible="false" />
                     </children>
                  </Pane>
               </children>
            </HBox>
         </children>
      </VBox>
    </children>
</fx:root>
