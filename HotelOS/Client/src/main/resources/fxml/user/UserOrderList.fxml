<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<fx:root prefHeight="690.0" prefWidth="850.0" styleClass="panel" stylesheets="@../../css/hotelworker/hotelworkerstyle.css" type="Pane" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.userui.usercontroller.UserOrderListController">
   <children>
      <TableView fx:id="orderList" layoutX="70.0" layoutY="170.0" prefHeight="464.0" prefWidth="711.0">
        <columns>
          <TableColumn fx:id="orderIDCol" prefWidth="190.0" resizable="false" styleClass="LeftAlignCol" text="订单号" />
          <TableColumn fx:id="orderTimeCol" minWidth="0.0" prefWidth="170.0" resizable="false" styleClass="LeftAlignCol" text="生成时间" />
            <TableColumn fx:id="orderStateCol" prefWidth="123.0" resizable="false" styleClass="LeftAlignCol" text="订单状态" />
            <TableColumn fx:id="hotelNameCol" prefWidth="141.0" resizable="false" styleClass="LeftAlignCol" text="酒店名称" />
            <TableColumn fx:id="btnCol" minWidth="0.0" prefWidth="60.0" resizable="false" />
        </columns>
      </TableView>
      <HBox fx:id="listTopBar" alignment="CENTER_LEFT" layoutX="70.0" layoutY="110.0" prefHeight="60.0" prefWidth="711.0" styleClass="listTopBar">
         <children>
            <ComboBox fx:id="orderStateComBox" prefHeight="36.0" prefWidth="171.0" promptText="-订单状态-">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ComboBox>
            <StackPane alignment="CENTER_RIGHT">
               <children>
                  <TextField id="searchField" fx:id="searchField" prefHeight="36.0" prefWidth="222.0" promptText="订单号">
                     <padding>
                        <Insets left="8.0" right="30.0" />
                     </padding>
                     <StackPane.margin>
                        <Insets />
                     </StackPane.margin>
                  </TextField>
                  <Button id="searchOrderIDBtn" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#searchOrderByID" prefHeight="25.0" prefWidth="25.0">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../img/hotelworker/searchInField.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <StackPane.margin>
                        <Insets right="5.0" />
                     </StackPane.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets left="280.0" />
               </HBox.margin>
            </StackPane>
         </children>
      </HBox>
   </children>
</fx:root>
