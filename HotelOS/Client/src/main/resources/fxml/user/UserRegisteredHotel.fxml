<fx:root type="Pane" prefHeight="690.0" prefWidth="850.0" stylesheets="@../../css/user/userstyle.css" fx:controller="presentation.userui.usercontroller.RegisteredHotelController" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox id="topBar" prefHeight="50.0" prefWidth="850.0" stylesheets="@../../css/user/userstyle.css">
         <children>
            <Button mnemonicParsing="false" stylesheets="@../../css/user/userstyle.css">
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../img/common/back.png" />
                     </image>
                  </ImageView>
               </graphic>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/common/initialPhoto.png" />
               </image>
               <HBox.margin>
                  <Insets left="480.0" top="10.0" />
               </HBox.margin>
            </ImageView>
            <SplitMenuButton maxWidth="120.0" mnemonicParsing="false" text="name">
