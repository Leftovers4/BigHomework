            </SplitMenuButton>
            <Button onAction="#minWindow" mnemonicParsing="false" stylesheets="@../../css/user/userstyle.css">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
